<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"

    android:layout_height="wrap_content">


    <LinearLayout
        android:visibility="gone"

        android:id="@+id/latestlayout"


        android:orientation="vertical"
        android:background="@drawable/track_bg1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <RelativeLayout
            android:layout_marginBottom="17.5dp"
            android:layout_marginTop="18.5dp"

            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/deliver_img"
                android:layout_marginStart="12.5dp"

                android:src="@drawable/checkcircke"
                android:layout_width="20dp"
                android:layout_height="20dp"/>


            <LinearLayout

                android:layout_alignLeft="@+id/deliver_img"





                android:layout_marginStart="32dp"

                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/latestatus"
                    android:textColor="#DE000000"
                    android:fontFamily="@font/open_sans"
                    android:textSize="15.8sp"

                    android:height="24dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.015"
                    android:lineSpacingMultiplier="1.52"
                    android:text="Delivered"
                    android:width="195dp" />


                <TextView
                    android:id="@+id/latest_date"
                    android:layout_marginBottom="5dp"
                    android:textColor="#99000000"
                    android:textSize="11.85sp"
                    android:fontFamily="@font/open_sans"
                    android:height="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.04"
                    android:lineSpacingMultiplier="1.35"
                    android:text="Mon Apr 18 2022 • 06:08 PM"
                    android:width="177dp" />



            </LinearLayout>

            <ImageView
                android:id="@+id/actionupandown"
                android:layout_marginEnd="16.5dp"
                android:layout_alignParentEnd="true"

                android:layout_gravity="end"



                android:src="@drawable/baseline_keyboard_arrow_down_24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:visibility="gone"
                android:id="@+id/actionupandown1"
                android:layout_marginEnd="16.5dp"
                android:layout_alignParentEnd="true"

                android:layout_gravity="end"



                android:src="@drawable/baseline_keyboard_arrow_up_24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>



        </RelativeLayout>




        <LinearLayout

            android:visibility="gone"

            android:id="@+id/indetails_lay"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <View
                android:background="#1F000000"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>


            <TextView
                android:visibility="gone"
                android:textColor="#DE000000"
                android:textSize="13.82sp"
                android:fontFamily="@font/open_sans"
                android:layout_marginTop="16.5dp"
                android:layout_marginStart="16.5dp"

                android:height="20dp"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:letterSpacing="0.025"
                android:lineSpacingMultiplier="1.45"
                android:text="Carrier:"
                android:width="116dp" />


            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/bintext"
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="16.5dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="Bin:"
                    />

                <TextView
                    android:id="@+id/binvalue"
                    android:layout_alignEnd="@+id/bintext"
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:height="20dp"
                    android:layout_marginStart="10dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="bin1"
                    android:width="116dp" />
            </LinearLayout>



            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="16.5dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="Storage Location:"
                    />

                <TextView
                    android:id="@+id/locationvalue"
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="10dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="Loc1"
                    android:width="116dp" />

            </LinearLayout>


            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="16.5dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="Dock:"
                    />

                <TextView
                    android:id="@+id/docvalue"
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="10dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:width="116dp" />
            </LinearLayout>



            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="16.5dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="Signed By:" />

                <TextView
                    android:id="@+id/signedby"
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="10dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="test"
                    android:width="116dp" />

            </LinearLayout>



            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:textColor="#DE000000"
                    android:textSize="13.82sp"
                    android:fontFamily="@font/open_sans"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="16.5dp"
                    android:height="20dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.025"
                    android:lineSpacingMultiplier="1.45"
                    android:text="Carrier Signature:" />

                <ImageView

                    android:layout_marginStart="10dp"


                    android:id="@+id/signImage"

                    android:layout_width="116dp"
                    android:layout_height="40dp"
                    android:gravity="center_vertical"/>

            </LinearLayout>




            <TextView
                android:layout_marginBottom="16dp"
                android:textColor="#DE000000"
                android:textSize="13.82sp"
                android:fontFamily="@font/open_sans"
                android:layout_marginTop="16dp"
                android:layout_marginStart="16.5dp"
                android:height="20dp"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:letterSpacing="0.025"
                android:lineSpacingMultiplier="1.45"
                android:text="Picture:"
                android:width="116dp" />

        </LinearLayout>




    </LinearLayout>


    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/seemorelayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:layout_marginStart="20dp"
            android:src="@drawable/progressdotbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>



        <LinearLayout
            android:visibility="gone"
            android:layout_marginStart="20dp"
            android:id="@+id/line_lay1"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#BEE3F2"/>

            <TextView
                android:textSize="25sp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:text=""
                android:textColor="@color/purple_200"/>


        </LinearLayout>

        <RelativeLayout
            android:layout_marginStart="16dp"
            android:layout_below="@+id/line_lay1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:visibility="gone"
                android:layout_marginTop="6dp"


                android:textSize="28dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u2022"
                android:textColor="@color/purple_200"/>

            <LinearLayout

                android:layout_marginTop="20dp"

                android:layout_marginStart="32dp"


                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <TextView
                    android:visibility="gone"
                    android:textColor="#DE000000"
                    android:fontFamily="@font/open_sans"
                    android:textSize="15.8sp"

                    android:height="24dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.015"
                    android:lineSpacingMultiplier="1.52"
                    android:text="On Rack"
                    android:width="195dp" />

                <TextView
                    android:layout_marginTop="14dp"
                    android:id="@+id/seemore1"
                    android:textColor="@color/purple_700"
                    android:textSize="11.85sp"
                    android:fontFamily="@font/open_sans"
                    android:height="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.04"
                    android:lineSpacingMultiplier="1.35"
                    android:text="See more updates"
                    android:width="180dp" />


                <TextView
                    android:visibility="gone"
                    android:textColor="#99000000"
                    android:textSize="11.85sp"
                    android:fontFamily="@font/open_sans"
                    android:height="16dp"
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:letterSpacing="0.04"
                    android:lineSpacingMultiplier="1.35"
                    android:text="Mon Apr 18 2022 • 06:08 PM"
                    android:width="177dp" />



            </LinearLayout>


        </RelativeLayout>

    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/defaultlayout"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <ImageView
        android:layout_marginStart="20dp"
        android:src="@drawable/progressdotbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>



        <LinearLayout
            android:visibility="gone"
            android:layout_marginStart="20dp"
            android:id="@+id/line_lay"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#BEE3F2"/>

            <TextView
                android:textSize="25sp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:text=""
                android:textColor="@color/purple_200"/>


        </LinearLayout>

    <RelativeLayout
        android:layout_marginStart="16dp"
        android:layout_below="@+id/line_lay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:visibility="gone"
            android:layout_marginTop="6dp"


            android:textSize="28dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="\u2022"
            android:textColor="@color/purple_200"/>

        <LinearLayout

            android:layout_marginTop="20dp"

            android:layout_marginStart="32dp"


            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <TextView
                android:id="@+id/statusTv"
                android:textColor="#DE000000"
                android:fontFamily="@font/open_sans"
                android:textSize="15.8sp"

                android:height="24dp"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:letterSpacing="0.015"
                android:lineSpacingMultiplier="1.52"
                android:text="On Rack"
                android:width="195dp" />

            <TextView
                android:visibility="gone"
                android:layout_marginTop="14dp"
                android:id="@+id/seemore"
                android:textColor="@color/purple_700"
                android:textSize="11.85sp"
                android:fontFamily="@font/open_sans"
                android:height="16dp"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:letterSpacing="0.04"
                android:lineSpacingMultiplier="1.35"
                android:text="See 3 more updates"
                android:width="180dp" />


            <TextView
                android:id="@+id/date_tv"
                android:textColor="#99000000"
                android:textSize="11.85sp"
                android:fontFamily="@font/open_sans"
                android:height="16dp"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:letterSpacing="0.04"
                android:lineSpacingMultiplier="1.35"
                android:text="Mon Apr 18 2022 • 06:08 PM"
                android:width="177dp" />



        </LinearLayout>


    </RelativeLayout>

    </RelativeLayout>




</LinearLayout>